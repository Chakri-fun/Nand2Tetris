/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP myMux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    myNot(in=sel,out=notSel);
    myAnd(a=a,b=notSel,out=out1);
    myAnd(a=b,b=sel,out=out2);
    myOr(a=out1,b=out2,out=out);
}
